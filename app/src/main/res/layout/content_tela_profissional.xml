<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="tcc.usjt.felix113.View.ViewProfissional.TelaProfissional"
    tools:showIn="@layout/app_bar_tela_profissional"
    android:orientation="vertical">

    <TextView
        android:id="@+id/txtContentTelaProfissional"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:text="Pessoas que estão procurando os seus Serviços "
        android:textAlignment="center"
        android:textColor="#000000" />

    <ListView
        android:id="@+id/ListViewConteTelaProfissional"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="match_parent" />


</LinearLayout>
